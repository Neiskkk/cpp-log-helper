{
  "name": "cpp-log-helper",
  "displayName": "cpp-log-helper",
  "description": "Insert std::cout log for selected variable",
  "publisher": "anast",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "cppLogHelper.insertCout",
        "title": "C++ Log Helper: Insert std::cout"
      }
    ],
    "keybindings": [
    {
      "command": "cppLogHelper.insertCout",
      "key": "ctrl+alt+n",
      "when": "editorTextFocus"
    }
  ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "1.104.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.0.0",
    "typescript-eslint": "^8.46.3",
    "eslint": "^9.39.1",
    "typescript": "^5.0.0",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}
